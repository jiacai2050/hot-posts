<table role="presentation" border="0" cellpadding="0" cellspacing="0">
<caption><h2>Hacker News</h2></caption>
<tr><th>Score</th><th>Title</th></tr>
<%  for(let post of posts) { %>
<tr>
    <td><a href="https://news.ycombinator.com/item?id=<%- post.id %>"><%= post.score %></a></td>
    <td><p><a href="<%- post.url %>"><%= post.title %></a></p></td>
</tr>
<%  }  %>

</table>


<% if (!!git_sha) { %>
<center>
Built With ❤️  by <a href="https://github.com/jiacai2050/v2ex/tree/<%= git_sha %>">jiacai2050/v2ex</a>
</center>
<%  }  %>
